{
	"name": "List Issues",
	"description": "List all issues in my repos",
	"version": "2.0",
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": {
			"19": "images/icon19.png",
			"38": "images/icon38.png"
		},
		"default_title": "List all issues in my repos"
	},
	"manifest_version": 2,
	"permissions": [
		"https://github.com/*"
	]
}